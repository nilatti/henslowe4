<%= semantic_form_for @rehearsal do |f| %>
  <%= f.inputs do %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :act %>
    <%= f.input :scene %>
    <%= f.input :french_scene %>
    <%= f.input :space %>
    <%= f.input :production %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
