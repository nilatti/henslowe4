<p id="notice"><%= notice %></p>
<% if @conflict.user %>
  <p>
    <strong>User:</strong>
    <%= @conflict.user.name %>
  </p>
<% end %>
<% if @conflict.space %>
  <p>
    <strong>Space:</strong>
    <%= @conflict.space.name %>
  </p>
<% end %>

<p>
  <strong>Start:</strong>
  <%= @conflict.start_time %>
</p>

<p>
  <strong>End:</strong>
  <%= @conflict.end_time %>
</p>

<p>
  <strong>Category:</strong>
  <%= @conflict.category %>
</p>

<%= link_to 'Edit', edit_conflict_path(@conflict) %> |
<%= link_to 'Back', conflicts_path %>
