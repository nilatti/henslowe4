<%= bootstrap_form_for @theater do |f| %>
    <%= f.text_field :name %>
    <%= f.text_field :street_address %>
    <%= f.text_field :city %>
    <%= f.text_field :state %>
    <%= f.text_field :zip %>
    <%= f.text_field :phone_number %>
    <%= f.text_field :mission_statement %>
    <%= f.text_field :website %>
    <%= f.text_field :calendar %>
    <%= f.submit %>
<% end %>
